{
  "name": "api-gateway",
  "version": "0.0.1",
  "description": "API Gateway",
  "main": "src/index.ts",
  "engines": {
    "node": "=14.5.0",
    "npm": "=6.14.5"
  },
  "scripts": {
    "requirements-check": "node --experimental-json-modules --no-warnings --trace-warnings check-version.js",
    "postinstall": "npm run requirements-check",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "test": "jest",
    "ts:dev": "ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "ts:prod": "tsc && node ./build/index.js",
    "prebuild": "npm run lint:fix",
    "prestart": "npm run build",
    "debug": "nodemon --inspect-brk=9229 src/index.ts",
    "dev": "nodemon --inspect=9229",
    "build": "tsc -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anushshukla/node-api-gateway.git"
  },
  "keywords": [
    "api",
    "gateway"
  ],
  "author": "Anush Shukla",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anushshukla/node-api-gateway/issues"
  },
  "homepage": "https://github.com/anushshukla/node-api-gateway#readme",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/express-rate-limit": "^5.0.0",
    "@types/express-sslify": "^1.2.1",
    "@types/hapi__joi": "^17.1.3",
    "@types/helmet": "0.0.47",
    "@types/multer": "^1.4.3",
    "@types/node": "^14.0.22",
    "@types/redis": "^2.8.22",
    "@types/supertest": "^2.0.10",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "express-sslify": "^1.2.0",
    "helmet": "^3.23.3",
    "http-proxy": "^1.18.1",
    "http-proxy-middleware": "^1.0.4",
    "load-balancers": "^1.3.52",
    "multer": "^1.4.2",
    "mysql2": "^2.1.0",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.25",
    "typescript": "^3.9.6"
  },
  "devDependencies": {
    "@typeform/eslint-config": "^1.0.5",
    "@types/body-parser": "^1.19.0",
    "@types/crypto-js": "^3.1.47",
    "@types/jest": "^26.0.3",
    "@types/semver": "^7.3.1",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "eslint": "^7.4.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-typeorm": "0.0.19",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "semver": "^7.3.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.1",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.51",
    "tslint": "^5.12.1"
  }
}
